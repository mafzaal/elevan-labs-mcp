# ElevenLabs MCP Server Configuration
# Copy this file to .env and fill in your values

# Required: Your ElevenLabs API Key
# Get it from: https://elevenlabs.io/app/speech-synthesis
ELEVENLABS_API_KEY=your_api_key_here

# Optional: Override default settings
# ELEVENLABS_DEFAULT_VOICE_ID=JBFqnCBsd6RMkjVDRZzb
# ELEVENLABS_DEFAULT_MODEL_ID=eleven_multilingual_v2
# ELEVENLABS_DEFAULT_OUTPUT_FORMAT=mp3_44100_128
# ELEVENLABS_OUTPUT_DIR=./audio_output
# ELEVENLABS_MAX_TEXT_LENGTH=5000
# ELEVENLABS_ENABLE_LOGGING=true
